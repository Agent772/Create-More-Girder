{
  "multipart": [
    {
      "apply": {
        "model": "createmoregirder:block/weathered_iron_girder_encased_shaft/block"
      },
      "when": {
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "createmoregirder:block/weathered_iron_girder_encased_shaft/block",
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "createmoregirder:block/weathered_iron_girder_encased_shaft/block_top"
      },
      "when": {
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "createmoregirder:block/weathered_iron_girder_encased_shaft/block_bottom"
      },
      "when": {
        "bottom": "true"
      }
    }
  ]
}